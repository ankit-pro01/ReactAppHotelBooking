function getParaIndex() {
 var paraIndex = -1;
 $('div.pdisc p').each(function(index){
   var textLength = $(this).text().length;
   if (textLength >= 250) {
     paraIndex = $(this).index();
     return false;
   }
 });
 return paraIndex;
}

